CA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Screenfull.vue","sourceRoot":"src/components/layout","sourcesContent":["<template>\n  <div class=\"container\" title=\"全屏/正常\">\n    <i class=\"iconfont\" :class=\"isFullscreen ? 'icon-quxiaoquanping' : 'icon-quanping'\" @click=\"handleFullScreen\"></i>\n  </div>\n</template>\n\n<script>\nimport screenfull from 'screenfull'\n\nexport default {\n  data() {\n    return {\n      isFullscreen: false,\n    }\n  },\n  mounted() {\n    this.init()\n  },\n  beforeDestroy() {\n    this.destroy()\n  },\n  methods: {\n    handleFullScreen() {\n      if (!screenfull.enabled) {\n        this.$message({\n          message: 'you browser can not work',\n          type: 'warning',\n        })\n        return false\n      }\n      screenfull.toggle()\n    },\n    change() {\n      this.isFullscreen = screenfull.isFullscreen\n    },\n    init() {\n      if (screenfull.enabled) {\n        screenfull.on('change', this.change)\n      }\n    },\n    destroy() {\n      if (screenfull.enabled) {\n        screenfull.off('change', this.change)\n      }\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.container {\n  width: 40px;\n  height: 40px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  margin-right: 10px;\n  cursor: pointer;\n\n  i {\n    width: 40px;\n    height: 40px;\n    font-size: 20px;\n\n    &:before {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      width: 40px;\n      height: 40px;\n    }\n  }\n}\n</style>\n"]}]}